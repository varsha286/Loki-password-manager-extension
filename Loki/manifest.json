{
    "manifest_version": 3,
    "name": "Password Manager",
    "version": "1.0",
    "description": "A simple password manager bookmark extension",
    "permissions": ["storage"],
    "action": {
        "default_popup": "src/auth.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
